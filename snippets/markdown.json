{
  "function": {
    "prefix": "func",
    "body":["[ `${1:CODE}` ]$0"]
  },
  "italic": {
    "prefix": "itaic",
    "body":["_${1:CODE}_ $0"]
  },
  "bold": {
    "prefix": "bold",
    "body":["**${1:CODE}** $0"]
  },
  "bold_itailc": {
    "prefix": "bold_italic",
    "body":["***${1:CODE}*** $0"]
  },
  "line": {
    "prefix": "line",
    "body":["----\n$0"]
  },
  "C Code": {
    "prefix": "code",
    "body": [
        "```${0:text}",
        "${1:CODE}",
        "```"
    ]
  },
  "Fold": {
    "prefix": "details",
    "body": [
        "<details>",
        "<summary>${1:SUMMARY}</summary><br />",
        "$0",
        "</details>"
    ]
  }
}
