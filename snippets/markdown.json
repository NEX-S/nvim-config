{
  "function": {
    "prefix": "func",
    "body":["[ `${1:CODE}` ]$0"]
  },
  "italic": {
    "prefix": "itaic",
    "body":["_${1:CODE}_ $0"]
  },
  "bold": {
    "prefix": "bold",
    "body":["**${1:CODE}** $0"]
  },
  "bold_itailc": {
    "prefix": "bold_italic",
    "body":["***${1:CODE}*** $0"]
  },
  "line": {
    "prefix": "l",
    "body":["----\n$0"]
  },
  "Code Block": {
    "prefix": "block",
    "body": [
        "```${1:text}",
        "${0:CODE}",
        "```"
    ]
  },
  "Code": {
    "prefix": "code",
    "body": "<code>${0:CODE}</code>"
  },
  "Space": {
    "prefix": "nbsp",
    "body": "&nbsp;"
  },
  "Tab": {
    "prefix": "emsp",
    "body": "&emsp;"
  },
  "bold": {
    "prefix": "space",
    "body": "&nbsp;"
  },
  "Fold": {
    "prefix": "details",
    "body": [
        "<details>",
        "<summary>${1:SUMMARY}</summary><br />",
        "",
        "$0",
        "",
        "</details>"
    ]
  }
}
